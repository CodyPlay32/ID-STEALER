function CorrectKeyNotification()
     OrionLib:MakeNotification({
        Name = "Notificaci√≥n",
        Content = "Contrase√±a correcta, cargando men√∫!  ‚úÖ",
        Image = "rbxassetid://7733666258",
        Time = 5
        })
    end
    
function IncorrectKeyNotification()
    OrionLib:MakeNotification({
        Name = "Notificaci√≥n",
        Content = "Contrase√±a incorrecta, lo siento!  ‚ùå",
        Image = "rbxassetid://7733666258",
        Time = 5
        })
    end
    
    local Key = Window:MakeTab({
        Name = "Clave",
        Icon = "rbxassetid://7733965118",
        PremiumOnly = false
    })

    local CreditsKey = Window:MakeTab({
        Name = "Creditos",
        Icon = "rbxassetid://7743876142",
        PremiumOnly = false
    })
    
    Key:AddTextbox({
        Name = "Introduzca clave",
        Default = "",
        TextDisappear = true,
        Callback = function(Value)
            if Value and Value ~= "" then
            if Value == _G.Key then
              CorrectKeyNotification()
              wait(6)
              MakeScriptHub()
          else
              IncorrectKeyNotification()
          end
       end
    end
  })

  CreditsKey:AddParagraph("LIBRARY - V5","Desarrollado Por CodyPlay32")
    

OrionLib:Init()
